.primary-nav {
  
  &--dkGold {
    a {
      color: $dkGold;
    }
  }
  &--dkBlue {
    a {
      color: $dkBlue;
    }
  }

  ul {
    display: flex;
    flex-direction: row;
    margin: 0;
    padding: 12px 0;

  }

  li {
  list-style: none;
  padding-right: 20px;
  }

  a {
    text-decoration: none;
    font-weight: 400;
  }

  ul ul {
    display: none;
  }

  


}